{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "node",
        "lib": [
            "ES2020"
        ],
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "declaration": true,
        "outDir": ".vercel/output/functions",
        "rootDir": ".",
        "types": [
            "node"
        ]
    },
    "include": [
        "api/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}